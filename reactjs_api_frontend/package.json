{
  "version": "1.2.8",
  "main": "src/index.js",
  "private": true,
  "repository": {
    "type": "git",
  },
  "engines": {
    "node": "7.x"
  },
  "config": {
    "path_docs": "docs",
    "path_src": "src",
    "path_assets": "assets",
  },
  "scripts": {
    "dev-webpack-build": "rimraf dist && ./node_modules/.bin/webpack --progress --config dll.webpack.config.js && babel-node ./node_modules/.bin/webpack --progress --config development.webpack.config.js",
    "dev-webpack-server": "rimraf dist && ./node_modules/.bin/webpack --progress --config dll.webpack.config.js && babel-node ./node_modules/.bin/webpack-dev-server --progress --config development.webpack.config.js",
    "dev-server": "babel-node src/server.js",
    "doc": "documentation build $npm_package_config_path_src/**/* $npm_package_config_path_assets/**/* -f html -o $npm_package_config_path_docs --name $npm_package_config_title -a public -a undefined -a private -c documentation.yml",
    "open-doc": "npm run -s doc && opn $npm_package_config_path_docs/index.html",
    "eslint": "eslint --ext .jsx --ext .js assets src",
    "test": "env-cmd test jest --config ./jest.json",
    "build-preproduction": "env-cmd preproduction ./tools/build.sh",
    "_build": "./tools/build.sh",
    "_clean-build-for-prod": "./tools/clean-build-for-prod.sh",
    "build": "npm run -s eslint -- --quiet && env-cmd production ./tools/build.sh && env-cmd production ./tools/clean-build-for-prod.sh"
  },
  "devDependencies": {
    "ajv": "^4.11.5",
    "axios": "^0.16.0",
    "babel-cli": "^6.24.0",
    "babel-core": "^6.24.0",
    "babel-eslint": "^7.2.1",
